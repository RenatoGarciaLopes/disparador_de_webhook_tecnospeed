{
  "name": "disparador_de_webhook_tecnospeed",
  "version": "1.0.0",
  "description": "Sistema disparador de webhooks para TecnSpeed",
  "scripts": {
    "dev": "tsx watch --env-file=.env ./src/server.ts",
    "build": "tsc",
    "start": "node ./dist/server.js",
    "test": "jest",
    "test:unit": "jest",
    "test:integration": "sh ./scripts/test-integration.sh",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "sequelize:migrate": "sequelize-cli db:migrate",
    "sequelize:seed": "sequelize-cli db:seed:all"
  },
  "keywords": [
    "webhook",
    "tecnospeed",
    "typescript",
    "node"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.11.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "redis": "^5.8.2",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.3.0",
    "sequelize-typescript": "^2.1.6",
    "uuid": "^11.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@types/express": "^4.17.23",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.11",
    "@types/supertest": "^6.0.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RenatoGarciaLopes/disparador_de_webhook_tecnospeed.git"
  },
  "bugs": {
    "url": "https://github.com/RenatoGarciaLopes/disparador_de_webhook_tecnospeed/issues"
  },
  "homepage": "https://github.com/RenatoGarciaLopes/disparador_de_webhook_tecnospeed#readme"
